Scalefreenetwork<-function(p){#
#
g<-barabasi.game(p,directed=FALSE,power=1.2)#
adjacency<-get.adjacency(g,type=c("both"))#
adjacency<-as.matrix(adjacency)#
temp<-adjacency#
