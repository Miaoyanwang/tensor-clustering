#
Theta<-(adjacency+t(adjacency))/2#
ee<-min(eigen(Theta,only.values=T)$values)#
diag(Theta)<-ifelse(ee < 0, -ee + 0.1, 0.1)#
#
return(list(Theta=Theta,hubcol=hub,adjacency=temp))#
}
